d+/$'): 'Delete role',\n    # Teams\n    ('GET', r'^/api/team/$'): 'List teams',\n    ('POST', r'^/api/team/$'): 'Create team',\n    ('GET', r'^/api/team/\\d+/$'): 'List team',\n    ('PATCH', r'^/api/team/\\d+/$'): 'Update team',\n    ('PUT', r'^/api/team/\\d+/$'): 'Update team',\n    ('DELETE', r'^/api/team/\\d+/$'): 'Delete team',\n    # Users\n    ('GET', r'^/api/users/$'): 'List users',\n    ('POST', r'^/api/users/$'): 'Create user',\n    ('GET', r'^/api/users/(?P<id>[^/]*-[^/]*)/$'): 'List user',\n    ('PATCH', r'^/api/users/(?P<id>[^/]*-[^/]*)/$'): 'Update user',\n    ('PUT', r'^/api/users/(?P<id>[^/]*-[^/]*)/$'): 'Update user',\n    ('DELETE', r'^/api/users/(?P<id>[^/]*-[^/]*)/$'): 'Delete user',\n    # Permissions by role\n    ('GET', r'^/api/permissions/$'): 'List permissions',\n    # User companies\n    ('GET', r'^/api/usercompanies/$'): 'List user companies',\n    ('POST', r'^/api/usercompanies/$'): 'Create user company',\n    ('GET', r'^/api/usercompanies/(?P<id>[^/]*-[^/]*)/$'): 'List user company',\n    ('PATCH', r'^/api/usercompanies/(?P<id>[^/]*-[^/]*)/$'): 'Update user company',\n    ('PUT', r'^/api/usercompanies/(?P<id>[^/]*-[^/]*)/$'): 'Update user company',\n    ('DELETE', r'^/api/usercompanies/(?P<id>[^/]*-[^/]*)/$'): 'Delete user company',\n}", "title": "API Path and Method to Action Dict", "nodeClass": "inputscalar", "moduleId": "_root_", "x": 40, "y": 130, "z": 1, "w": "180", "h": "30", "description": null, "units": null, "color": null, "errorInDef": false, "nodeInfo": {"showInputs": 1, "showOutputs": 1, "showLabel": 1, "showBorder": 1, "fill": 1, "useNodeFont": 0}, "nodeFont": null, "numberFormat": null, "originalId": null, "extraData": null, "picture": null, "evaluateOnStart": false, "releaseMemory": false, "defaultView": null, "prefixIdentifier": null, "hierarchy": null, "hasOutputsOutsideModule": false, "_isNodeCircular": false, "_resultType": "dict", "metadata": {"created_at": "2025-04-10T15:01:40.681664+00:00", "last_modified_at": "2025